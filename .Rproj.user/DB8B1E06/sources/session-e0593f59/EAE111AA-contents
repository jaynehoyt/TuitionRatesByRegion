\hypertarget{performing-two-sample-t-tests}{%
\section{Performing Two-Sample
T-Tests}\label{performing-two-sample-t-tests}}

\hypertarget{southwest-v-west}{%
\subsection{Southwest v West}\label{southwest-v-west}}

\begin{Shaded}
\begin{Highlighting}[]
\CommentTok{\#Southwest v West}
\CommentTok{\# Null : Southwest = West}
\CommentTok{\# Alternative: Southwest \textless{} West }

 \CommentTok{\#(beta1\_\{group1\} {-} beta1\_\{group2\})/sqrt(se(beta1\_\_\{group1\})\^{}2 + se(beta1\_\{group2\})\^{}2)}

\NormalTok{t }\OtherTok{=}\NormalTok{ (}\FloatTok{344.9} \SpecialCharTok{{-}} \FloatTok{375.97}\NormalTok{) }\SpecialCharTok{/} \FunctionTok{sqrt}\NormalTok{((}\FloatTok{9.272}\NormalTok{)}\SpecialCharTok{\^{}}\DecValTok{2} \SpecialCharTok{+}\NormalTok{ (}\FloatTok{21.01}\NormalTok{)}\SpecialCharTok{\^{}}\DecValTok{2}\NormalTok{)}
\NormalTok{t}
\end{Highlighting}
\end{Shaded}

\begin{verbatim}
## [1] -1.35293
\end{verbatim}

Test Statistic = -1.353

Degrees of Freedom= 20

\begin{Shaded}
\begin{Highlighting}[]
\CommentTok{\#Find P{-}value}
\NormalTok{p }\OtherTok{=} \DecValTok{2}\SpecialCharTok{*}\FunctionTok{pt}\NormalTok{(t, }\DecValTok{20}\NormalTok{) }
\NormalTok{p }\OtherTok{=}\NormalTok{ p }\SpecialCharTok{*} \DecValTok{15} \CommentTok{\# Bonferonni adjustment}
\NormalTok{p}
\end{Highlighting}
\end{Shaded}

\begin{verbatim}
## [1] 2.867632
\end{verbatim}

P-value : 1

\emph{Not significantly different}

